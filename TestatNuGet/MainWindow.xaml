<Window x:Class="TestatNuGet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestatNuGet.ViewModel"
        xmlns:views="clr-namespace:TestatNuGet.View"
        mc:Ignorable="d"
        Title="TestatNuGet" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate DataType="{x:Type local:LogentriesViewModel}">
            <views:LogentriesView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:LogMessageAddViewModel}">
            <views:LogMessageAddView/>
        </DataTemplate>
    </Window.Resources>
    <ContentControl x:Name="Pages" Content="{Binding SelectedViewModel}"/>
</Window>
